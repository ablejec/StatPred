#abline(h=mean(y),lty=3)
#abline(v=mean(x),lty=3)
#points(mean(x),mean(y),cex=2,col=3,pch=16)
points(x,predict(lm(y~0+x*spol)),col=spol,pch=16)
@
\end{frame}
