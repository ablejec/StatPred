\section{Korelacija in regresija}

Slika Lenoarda da Vincija 'Virtruvian man' prikazuje razmerja razliènih delov telesa. Višina èloveka in
razpon rok naj bi bila približno enaka. Èe trditev drži, bi morale toèke doloèene z višino ($x$)
 in razponom rok ($y$) ležati na premici $y=x$.

%% |--------------------->>>>>>
\begin{frame}[fragile]
\frametitle{Virtruvian man: Roke = Višina}
<<fig=true,echo=false>>=
plot(visina, roke,col=spol)
abline(0,1,col="darkgreen",lwd=2)
@
\end{frame}
%% <<<<<<---------------------|

%% |--------------------->>>>>>
\begin{frame}[fragile]
\frametitle{Spremenljivke}
<<>>=
select <- roke>0
x <- visina[select]
y <- roke[select]
spol <- spol[select]
n <- length(x)

@
\end{frame}
%% <<<<<<---------------------|
%% |--------------------->>>>>>
\begin{frame}[fragile]
\frametitle{Osnovni raèun}
\vspace{-1cm}
<<>>=
(xbar <- mean(x))
(ybar <- mean(y))
sum((x-xbar)*(y-ybar))/(n-1)
cov(x,y)
(r <- cov(x,y)/(sd(x)*sd(y)))
r^2
(b <- cov(x,y)/var(x))
(a <- mean(y)-b*mean(x))
@
\end{frame}
%% <<<<<<---------------------|




%% |--------------------->>>>>>
\begin{frame}[fragile]
\frametitle{Virtruvian man: Roke = Višina}
<<fig=true,echo=false>>=
plot(x, y,col=spol,xlab="visina",ylab="roke")
abline(0,1,col="darkgreen",lwd=2)
abline(c(a,b),col="red",lwd=4)
abline(h=mean(y),lty=3)
abline(v=mean(x),lty=3)
#points(mean(x),mean(y),cex=2,col=3,pch=16)
@
\end{frame}
%% <<<<<<---------------------|

%% |--------------------->>>>>>
\begin{frame}[fragile]
\frametitle{Regresija v R}
\vspace{-1cm}
<<>>=
cor(x,y)
lsfit(x,y)$coefficients
lm(y~x)$coefficients
lm(y~x*spol)
@
\end{frame}
%% <<<<<<---------------------|

%% |--------------------->>>>>>
\begin{frame}[fragile]
\frametitle{Virtruvian man: Roke = Višina}
<<fig=true,echo=false>>=
plot(x,y,col=spol,xlab="visina",ylab="teza")
abline(0,1,col="darkgreen",lwd=2)
abline(c(a,b),col="red",lwd=4)
abline(lm(y~x),col="blue",lwd=1)
#abline(h=mean(y),lty=3)
#abline(v=mean(x),lty=3)
#points(mean(x),mean(y),cex=2,col=3,pch=16)
points(x,predict(lm(y~0+x*spol)),col=spol,pch=16)
@
\end{frame}
%% <<<<<<---------------------|

%% |--------------------->>>>>>
\begin{frame}[fragile]
\frametitle{Analiza variance za linearni model}
<<>>=
anova(lm(roke~visina*spol))
@
\end{frame}
%% <<<<<<---------------------|


