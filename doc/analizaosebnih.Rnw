\section{Kontingenca}

%% |--------------------->>>>>>
\begin{frame}[fragile]
\frametitle{Barva las in oèi}
<<>>=
f <- table(lasje,oci)
f
addmargins(f)
@
\end{frame}
%% <<<<<<---------------------|

%% |--------------------->>>>>>
\begin{frame}[fragile]
\frametitle{Prièakovane frekvence}
<<>>=
e <- outer(rowSums(f),colSums(f))/sum(f)
addmargins(e)

@
\end{frame}
%% <<<<<<---------------------|

\begin{frame}[fragile]
\frametitle{Razlika opaženega in prièakovanega}
<<>>=
f-e
(f-e)^2/e
1.96^2
@
\end{frame}

%% |--------------------->>>>>>
\begin{frame}[fragile]
\frametitle{Test $\chi^2$}
<<>>=
alpha <- 0.05
(df <- (ncol(f)-1)*(nrow(f)-1))
(X2.krit <- qchisq(1-alpha,df))
(X2 <- sum((f-e)^2/e))
(p <- 1-pchisq(X2,df))
if(X2<X2.krit) cat("Spremenljivki NISTA odvisni (p =",round(p,4),").\n") else
cat("Spremenljivki STA odvisni (p <",alpha,") (p =",round(p,4),").\n")
@
\end{frame}
%% <<<<<<---------------------|

%% |--------------------->>>>>>
\begin{frame}[fragile]
\frametitle{Funkcija za test v R}
<<>>=
chisq.test(lasje,oci,correct=FALSE)
@
\end{frame}
